package selection_Sort;

public class SelectionSort {
    static void selection_sort(int[] arr){
    	for(int i = 0; i<arr.length-1; i++){
    		int min = arr[0];
    		for(int j=i+1; j<arr.length; j++){
    			if(arr[i] < arr[j]){
    				//swap:
    				int temp = min;
    				int arr[i]= arr[j];
    				int arr[j] = min;
    			}
    		}
    	}
    }
	public static void main(String[] args) {
		int[] arr = {1,5,3,2,4};
		selection_sort(arr);

	}

}
