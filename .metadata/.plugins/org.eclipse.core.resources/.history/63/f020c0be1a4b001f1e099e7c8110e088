package geekForgeeksProblems;

public class MissingNumber {

	public static void main(String[] args) {
		int[] arr = {1,2,3,5};
		
		for(int i=0; i<arr.length; i++){
			//System.out.println(arr[i]);
		   int nexVal = i+1;
		   if(arr[i] != nexVal){
			   System.out.println(arr[i]);
			   break;
		   }
		
		}

	}

}
