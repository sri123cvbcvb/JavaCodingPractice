package geekForgeeksProblems;

public class BinarySearch {

	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5};
		int k = 4;
		
		int start = 0;
		int end = arr.length-1;
		
		System.out.println(mid);
		for(int i=0 ; i<arr.length; i++){
			int mid = (start+end)/2;
			if(arr[mid] == k){
				System.out.println(mid);
			}
			else if( k < arr[mid]){
				end = mid-1;
			}
			else{
				start = mid+1;
			}
		}

	}

}
